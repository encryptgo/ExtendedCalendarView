        android:layout_height="match_parent"/>

    <org.yohei.extendedcalendarview.widget.ECWeekItemView
        android:id="@+id/ecv__week_wed"
        week:week_value="@string/ecv__web"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <org.yohei.extendedcalendarview.widget.ECWeekItemView
        android:id="@+id/ecv__week_thu"
        week:week_value="@string/ecv__thu"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <org.yohei.extendedcalendarview.widget.ECWeekItemView
        android:id="@+id/ecv__week_fri"
        week:week_value="@string/ecv__fri"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <org.yohei.extendedcalendarview.widget.ECWeekItemView
        android:id="@+id/ecv__week_sat"
        week:week_value="@string/ecv__sat"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</merge>